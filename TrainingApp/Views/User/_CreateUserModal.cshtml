<div class="modal" id="createUserModal" tabindex="-1" role="dialog" aria-labelledby="createUserLabel" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="createUserLabel">Create User</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"
                        onclick="modalClose('createUserModal')">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <div class="form-group row">
                    <label for="templateName" class="col-sm-4 col-form-label">
                        First Name <span class="red">*</span>
                    </label>
                    <div class="col-sm-8">
                        <input type="text" class="form-control" id="templateName">
                    </div>
                </div>
                <div class="form-group row">
                    <label for="templateDescription" class="col-sm-4 col-form-label">
                        Last Name <span class="red">*</span>
                    </label>
                    <div class="col-sm-8">
                        <input type="text" class="form-control" id="templateDescription">
                    </div>
                </div>
                <div class="form-group row">
                    <label for="scriptNumber" class="col-sm-4 col-form-label">
                        User Name <span class="red">*</span>
                    </label>
                    <div class="col-sm-8">
                        <input type="text" class="form-control" id="scriptNumber">
                    </div>
                </div>
                <div class="form-group row">
                    <label for="attemptAllow" class="col-sm-4 col-form-label">
                        User Code
                    </label>
                    <div class="col-sm-8">
                        <input type="text" class="form-control mt10" id="userCode">
                    </div>
                </div>

                <div class="form-group">
                    <label class="col-form-label">
                        User Role
                    </label>
                    <div class="ml20">
                        <div class="form-check">
                            <input class="form-check-input" type="checkbox" value="" id="flexCheckDefault">
                            <label class="form-check-label" for="flexCheckDefault">
                                Admin
                            </label>
                        </div>
                        <div class="form-check">
                            <input class="form-check-input" type="checkbox" value="" id="flexCheckChecked">
                            <label class="form-check-label" for="flexCheckChecked">
                               Examiner
                            </label>
                        </div>
                        <div class="form-check">
                            <input class="form-check-input" type="checkbox" value="" id="flexCheckChecked">
                            <label class="form-check-label" for="flexCheckChecked">
                                Trainee
                            </label>
                        </div>

                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-primary button-left">Create</button>
                <button type="button" class="btn btn-light" onclick="modalClose('createUserModal')">Close</button>
            </div>
        </div>
    </div>
</div>
<div class="modal-backdrop hide" style="display:none"></div>